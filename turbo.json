{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env*"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**", "public/build/**", "dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
